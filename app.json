{
  "expo": {
    "name": "Timer",
    "description": "Simple app to time yourself.",
    "slug": "timer",
    "privacy": "public",
    "sdkVersion": "24.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#ffffff",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#195ba2",
      "hideExponentText": true
    },
    "notification": {
      "icon": "./assets/notifIcon.png",
      "color": "#00B300"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rodiwaa.timer"
    },
    "android": {
      "package": "com.rodiwaa.timer"
    }
  }
}
